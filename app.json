{
  "expo": {
    "name": "CrewMate",
    "slug": "CrewMate-App",
    "owner": "crewmate-app",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crewmateapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crewmateapp.CrewMateApp",
      "buildNumber": "19",
      "config": {
        "googleMapsApiKey": "AIzaSyD9SoERup6vjlVnlVvG89Bz2-YrZXGnMkI"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "CrewMate needs your location to verify you're in your layover city and connect you with nearby crew.",
        "NSLocationAlwaysUsageDescription": "CrewMate uses your location to connect you with nearby crew members during your layovers.",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ]
      },
      "associatedDomains": [
        "applinks:crewmate.app",
        "applinks:www.crewmate.app"
      ]
    },
    "android": {
      "versionCode": 15,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD9SoERup6vjlVnlVvG89Bz2-YrZXGnMkI"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.crewmateapp.CrewMateApp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "crewmate.app",
              "pathPrefix": "/plan"
            },
            {
              "scheme": "https",
              "host": "www.crewmate.app",
              "pathPrefix": "/plan"
            },
            {
              "scheme": "https",
              "host": "crewmate.app",
              "pathPrefix": "/connect"
            },
            {
              "scheme": "https",
              "host": "www.crewmate.app",
              "pathPrefix": "/connect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "CrewMate needs your location to verify you're in your layover city and connect you with nearby crew."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#2A4E9D",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "077aad83-fde5-4782-9b22-d67ff58f8b6f"
      }
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#2A4E9D",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "CrewMate"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/077aad83-fde5-4782-9b22-d67ff58f8b6f"
    }
  }
}
